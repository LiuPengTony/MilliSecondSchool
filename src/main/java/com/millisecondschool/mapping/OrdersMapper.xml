<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.millisecondschool.dao.OrdersMapper" >
  <resultMap id="BaseResultMap" type="com.millisecondschool.entity.Orders" >
    <result column="id" property="id" jdbcType="INTEGER" />
    <result column="OrderNo" property="orderno" jdbcType="VARCHAR" />
    <result column="Id_P" property="idP" jdbcType="INTEGER" />
  </resultMap>

  <sql id="Base_Column_List" >
    id, OrderNo, Id_P
  </sql>

  <select id="getThird" resultType="String">
    SELECT OrderNo FROM `orders` WHERE Id_P = 3;
  </select>
</mapper>